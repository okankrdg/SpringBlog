@model List<SpringBlog.Models.Category>
<ul class="list-unstyled mb-0">
    <li>
        @Html.RouteLink("All Categories","homedefault")
    </li>
    @foreach (var cat in Model)
    {
        <li>
            <a href="@Url.Action("Index","Home",new { cid=cat.Id, slug=cat.Slug})">@cat.CategoryName</a>
        </li>
    }
</ul>
